{
  "name": "spotify-utility",
  "version": "0.1.0",
  "description": "Personal Spotify Web API utility helpers.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check . --write --unsafe",
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "tsup"
  },
  "keywords": [
    "spotify",
    "utility",
    "web-api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@spotify/web-api-ts-sdk": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@types/node": "^24.0.15",
    "tsup": "^8.5.1",
    "typescript": "^5.8.3"
  }
}
